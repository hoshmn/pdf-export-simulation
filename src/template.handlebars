<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <style>
    @font-face {
      font-family: 'GT Eesti Display';
      font-weight: bold;
      font-style: normal;
      src: url("{{{assets.GTEestiDisplayBold}}}");
    }

    @font-face {
      font-family: 'Akkurat';
      font-weight: normal;
      font-style: normal;
      src: url({{assets.Akkurat}});
    }

    @font-face {
      font-family: 'Akkurat';
      font-weight: bold;
      font-style: normal;
      src: url({{assets.AkkuratBold}});
    }

    body {
      margin: 0;
    }

    .page {
      margin: 0;
      display: block;
      width: 612px;
      height: 792px;
      margin: 0;
    }

    p {
      font-family: 'Akkurat', serif;
      font-size: 10px;
      color: #666666;
      margin: 0;
    }

    #info,
    #date {
      position: absolute;
      left: 46px;
    }

    #info {
      bottom: 46px;
    }

    #date {
      bottom: 78px;
    }

    .page:first-of-type {
      top: 0;
      background: -moz-linear-gradient(top, rgba(255, 255, 255, 0) 0%, rgba(224, 65, 25, 0.15) 100%);
      background: -webkit-linear-gradient(top, rgba(255, 255, 255, 0) 0%, rgba(224, 65, 25, 0.15) 100%);
      background: linear-gradient(to bottom, rgba(255, 255, 255, 0) 0%, rgba(224, 65, 25, 0.15) 100%);
    }

    #logo {
      position: absolute;
      bottom: 46px;
      left: 484px;
      width: 82px;
      height: 79px;
    }

    #title {
      position: relative;
      left: 46px;
      top: 50%;
      transform: translateY(-50%);
      margin: 0;
    }

    #title .title-intro {
      font-family: 'GT Eesti Display', Helvetica, sans-serif;
      font-weight: bold;
      text-transform: uppercase;
      font-size: 13px;
      line-height: 24px;
      margin: 0;
      margin-bottom: 12px;
    }

    #title .locations {
      width: 520px;
      padding-right: 16px;
    }

    #title .locations h1 {
      font-family: 'GT Eesti Display', Helvetica, sans-serif;
      font-weight: bold;
      text-transform: uppercase;
      font-size: 36px;
      line-height: 48px;
      margin: 0;
    }

    #title .attribution {
      margin: 16px 0;
      font-size: 13px;
      line-height: 28px;
      font-family: 'Akkurat', Helvetica, sans-serif;
    }

    #title .locations h1:first-child {
      color: #E24000;
    }

    #title .locations h1:nth-child(2) {
      color: #434878
    }

    #title .locations h1:nth-child(3) {
      color: #2C897F;
    }

    /* #title .locations h1:nth-last-child(2)::after { content: ' &'; } */

    /* FEATURE DETAIL PAGE */
    .page.feature-details {
      position: relative;
    }

    .page.feature-details .screenshot {
      top: 46px;
      position: absolute;
      background-color: lightgrey;
      background-size: cover;
      background-repeat: no-repeat;
      background-position: center;
      left: 46px;
      width: 520px;
    }

    .page.feature-details .details {
      position: absolute;
      height: 103px;
      left: 46px;
      right: 46px;
    }

    .page.feature-details .footer {
      position: absolute;
      font-family: 'Akkurat', Helvetica, sans-serif;
      /* color: #B2B5B7; */
      left: 46px;
      top: 712px;
      padding-top: 6px;
      width: 520px;
      border-top: solid 1px #B2B5B7;
    }

    .page.feature-details .footer p {
      font-size: 8px;
      margin: 2px;
    }

    .page.feature-details .details h3 {
      margin: 0;
      font-family: 'GT Eesti Display', Helvetica, sans-serif;
      font-weight: bold;
      text-transform: uppercase;
    }

    .page.feature-details .details ul {
      padding-left: 16px;
      font-family: 'Akkurat', Helvetica, sans-serif;
      font-size: 11px;
      line-height: 17px;
      text-align: start;
      /* margin-bottom: 6px; */
    }

    .page.feature-details .details ul li span:last-child {
      font-weight: bold;
    }

    .page.feature-details .screenshot:first-child {
      top: 46px;
    }

    .page.feature-details .details:nth-child(2) h3 {
      color: #E24000;
    }

    .page.feature-details .details:nth-child(4) h3 {
      color: #434878;
    }

    .page.feature-details .details:nth-child(6) h3 {
      color: #2C897F;
    }

    /* MAP LEGEND */
    .map-legend {
      width: 112px;
      height: 48px;
      position: absolute;
      background-size: cover;
      background-repeat: no-repeat;
      background-position: center;
    }

    .map-legend.data-prop {
      width: 240px;
    }

    .screenshot .map-legend {
      bottom: 8px;
      right: 8px;
    }

    /* ONE FEATURE */
    .page.one-feature .screenshot {
      height: 520px;
    }

    .page.one-feature .details {
      top: 586px;
    }

    /* TWO FEATURES */
    .page.two-features .screenshot {
      height: 190px;
    }

    .page.two-features .details:nth-child(2) {
      top: 248px;
    }

    .page.two-features .screenshot:nth-child(3) {
      top: 384px;
    }

    .page.two-features .details:nth-child(4) {
      top: 586px;
    }

    /* THREE FEATURES */
    .page.three-features .screenshot {
      width: 200px;
      height: 200px;
    }

    .page.three-features .details {
      left: 281px;
    }

    .page.three-features .screenshot {
      height: 190px;
    }

    .page.three-features .details:nth-child(2) {
      top: 46px;
    }

    .page.three-features .screenshot:nth-child(3),
    .page.three-features .details:nth-child(4) {
      top: 269px;
    }

    .page.three-features .screenshot:nth-child(5),
    .page.three-features .details:nth-child(6) {
      top: 497px;
    }

    /* CHARTS PAGE */
    .page.charts {
      position: relative;
    }

    .page.charts .title {
      position: absolute;
      font-size: 15px;
      top: 60px;
      left: 100px;
      width: 416px;
      text-align: center;
      text-transform: uppercase;
      font-family: 'GT Eesti Display', Helvetica, sans-serif;
      font-weight: bold;
      color: #000000;
      z-index: 1;
    }

    .page.charts .chart {
      width: 425px;
      height: 520px;
      left: 96px;
      background-color: lightgrey;
      position: absolute;
      background-repeat: no-repeat;
      background-size: contain;
    }

    .page.charts .chart:first-of-type {
      top: 110px;
    }

    .page.charts .chart:nth-of-type(3) {
      top: 420px;
    }

    .page.charts .legend {
      position: absolute;
      left: 146px;
      width: 325px;
    }

    .page.charts .legend p {
      font-size: 14px;
      line-height: 22px;
      font-weight: bold;
    }

    .page.charts .legend.line {
      top: 500px;
    }

    .page.charts .legend.bar {
      top: 680px;
    }

    .legend p:first-child {
      color: #E24000;
    }

    .legend p:nth-child(2) {
      color: #434878;
    }

    .legend p:nth-child(3) {
      color: #2C897F;
    }

    .legend.us-avg p:last-child {
      color: #94aabd;
    }

    .legend.line p span:first-child {
      display: inline-block;
      background-repeat: no-repeat;
      background-position: center;
      background-size: contain;
      height: 14px;
      width: 30px;
      margin-right: 16px;
    }

    /* STATS TABLES */
    .page.stats {
      position: relative
    }

    .page.stats .stat-table {
      position: absolute;
      top: 46px;
      height: 716px;
      border: 1px solid #C6CCCF;
      display: inline-block;
    }

    .page.stats .stat-table h3 {
      margin: 8px;
      margin-top: 16px;
      font-size: 15px;
      font-family: 'GT Eesti Display', Helvetica, sans-serif;
      font-weight: bold;
    }

    .page.stats .stat-table p.year {
      font-size: 12px;
      font-weight: bold;
      font-family: 'Akkurat', Helvetica, sans-serif;
      color: #000;
      margin: 8px;
    }

    .page.stats .stat-table:first-child h3 {
      color: #E24000;
    }

    .page.stats .stat-table:nth-child(2) {
      color: #434878;
    }

    .page.stats .stat-table:nth-child(3) {
      color: #2C897F;
    }

    .page.stats.one-feature .stat-table {
      left: 130px;
      width: 350px;
      height: 716px;
    }

    .page.stats.two-features .stat-table {
      top: 97px;
      left: 60px;
      width: 230px;
      height: 600px;
    }

    .page.stats.two-features .stat-table:nth-child(2) {
      left: 325px;
    }

    .page.stats.three-features {
      transform: rotate(90deg) translate(90px, 90px);
      height: 612px;
      width: 792px;
      margin-bottom: 180px;
    }

    .page.stats.three-features .stat-table {
      margin-top: 0;
      width: 220px;
      height: 520px;
    }

    .page.stats.three-features .stat-table:first-child {
      margin-left: 36px;
      left: 0;
    }

    .page.stats.three-features .stat-table:nth-child(2) {
      left: 282px;
    }

    .page.stats.three-features .stat-table:nth-child(3) {
      left: 527px;
    }

    .prominent {
      width: 40%;
      margin: 24px 8px;
      text-align: center;
    }

    .prominent p {
      color: #000;
    }

    .prominent p:first-child {
      font-size: 15px;
      font-weight: bold;
    }

    .prominent.efpd {
      float: left;
    }

    .prominent.efr {
      float: right;
    }

    /* add all data-props, but hide the prominent ones  */
    .stat-table .data-props.efpd,
    .stat-table .data-props.efr {
      display: none;
    }

    .data-props {
      margin: 0 8px;
      padding: 2px;
      height: 20px;
      clear: both;
    }

    .data-props p {
      margin: 6px;
      color: #000;
    }

    .data-props p:first-child {
      float: left;
    }

    .data-props p:nth-child(2) {
      float: right;
      font-weight: bold;
    }

    .data-props:nth-child(even) {
      background-color: #ddd;
    }

    /* STYLES FOR UNAVAILABLE */
    .page.stats>.stat-table>.efpd.unavailable p:first-child,
    .page.stats>.stat-table>.efr.unavailable p:first-child,
    .page.stats .data-props>p:nth-child(2) {
      font-weight: normal;
      margin-left: 0;
    }

    .page.stats .data-props>p:first-child {
      width: 60%;
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
    }

    /* give median household income slight bump to prevent ellipsis */
    .page.stats .data-props.mhi>p:first-child {
      width: 65%;
    }

    /* give long-named % metrics bump to prevent ellipsis (the % won't take up much space) */
    .page.stats .data-props.tr>p:first-child,
    .page.stats .data-props.pro>p:first-child,
    .page.stats .data-props.pai>p:first-child,
    .page.stats .data-props.pnp>p:first-child {
      width: 73%;
    }

    p.divider {
      text-align: center;
      margin: 16px;
      font-weight: bold;
      color: #000;
    }

    .page.stats.one-feature .data-props {
      height: 24px;
    }

    .page.stats.one-feature .data-props p {
      font-size: 12px;
    }

    .page.stats.one-feature p.divider {
      margin: 24px;
      font-size: 14px;
    }

    .page.stats.two-features .efr,
    .page.stats.two-features .efpd {
      margin: 16px 8px;
    }

    .page.stats.two-features p.divider {
      margin: 12px;
    }

    .page.stats.two-features .data-props {
      padding: 1.5px;
    }

    .page.stats.three-features .data-props {
      height: 16px;
    }

    .page.stats.three-features .data-props p {
      margin: 2px;
    }

    .page.stats.three-features .efr,
    .page.stats.three-features .efpd {
      margin: 16px 8px;
    }

    .page.stats.three-features p.divider {
      margin: 12px;
    }

    /* REPORT PAGES */
    .page.report {
      position: relative;
    }

    .page.with-heading h1 {
      margin: 0 46px;
      padding-top: 46px;
      font-size: 30px;
      font-family: 'GT Eesti Display', Helvetica, sans-serif;
      margin-bottom: 32px;
    }

    .page.report #logo-top {
      position: absolute;
      top: 46px;
      right: 46px;
      width: 38px;
      height: 36px;
    }

    .page.report .section {
      margin: 0 46px;
      margin-bottom: 20px;
    }

    .page.report .section p {
      font-family: 'Akkurat', Helvetica, sans-serif;
      font-size: 12px;
      margin: 8px 0;
      color: #000;
      line-height: 16px;
    }

    .page.report .section p:first-child {
      font-weight: bold;
    }

    .page.report:not(.starts-with-heading) .section:first-of-type {
      padding-top: 80px;
    }

    .page.report .info {
      color: #666;
      margin: 32px 46px;
    }

    .low-flag,
    .high-flag {
      width: 13px;
      height: 13px;
      display: inline-block;
      vertical-align: sub;
    }

    .footer .low-flag,
    .footer .high-flag,
    .data-props .low-flag,
    .data-props .high-flag {
      width: 10px;
      height: 10px;
    }

    .low-flag {
      fill: #737373;
    }

    .high-flag {
      fill: #e24000;
    }
  </style>
</head>

